{
    "name": "jetimob/asaas-sdk-php-laravel",
    "description": "Laravel SDK for Asaa's API",
    "keywords": ["sdk", "laravel", "php", "asaas"],
    "type": "library",
    "authors": [
        {
            "name": "Gabriel Oliveira",
            "email": "oliveiragabriel.dev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "jetimob/http-php-laravel": "^2",
        "illuminate/support": "^9.0 || ^10.0",
        "illuminate/console": "^9.0 || ^10.0",
        "illuminate/container": "^9.0 || ^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.6.1 || ^8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jetimob\\Asaas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetimob\\Asaas\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jetimob\\Asaas\\AsaasServiceProvider"
            ],
            "aliases": {
                "Asaas": "Jetimob\\Asaas\\Facades\\Asaas"
            }
        }
    },
    "support": {
        "issues": "https://github.com/jetimob/asaas-sdk-php-laravel/issues"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
